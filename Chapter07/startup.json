  {
    "description":"这是一个带cookies信息+参数的post请求",
    "request":{
      "uri":"/post/with/cookies/params",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"xiaofeng",
        "age":"24"
      }
    },
    "response":{
      "status":"200",
      "json":{
        "success":"true",
        "message":"2"
      }
    }
  },
  {
    "description":"这是一个带header信息的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "headers":{
        "Content-Type":"application/json"
      },
      "json":{
        "name":"li",
        "age":"13"
      }
    },
    "response":{
      "json":{
        "status":"123",
        "message":"访问成功，这是一个带headers信息的post请求"
      }
    }
  }
